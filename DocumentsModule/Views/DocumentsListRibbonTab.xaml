<RibbonTab x:Class="DocumentsModule.Views.DocumentsListRibbonTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:DocumentsModule.Views"
             mc:Ignorable="d"
             xmlns:prism="http://prismlibrary.com/"
             prism:ViewModelLocator.AutoWireViewModel="True"
             Header="Dokumenty"
           xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
           IsSelected="{Binding IsTabSelected, Mode=TwoWay}">
    <RibbonGroup>
        <RibbonButton LargeImageSource="pack://application:,,,/Images/add.png" Label="Dodaj"/>
        <RibbonButton LargeImageSource="pack://application:,,,/Images/quit.png" Label="Zamknij" Command="{Binding ExitViewCommand}" />
    </RibbonGroup>
</RibbonTab>
